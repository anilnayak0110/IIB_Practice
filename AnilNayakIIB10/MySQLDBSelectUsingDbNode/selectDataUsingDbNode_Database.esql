
DECLARE MySQLDataSource EXTERNAL CHARACTER;
DECLARE TABLE EXTERNAL CHARACTER;
CREATE DATABASE MODULE selectDataUsingDbNode_Database
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		CALL SelectDataFromDB();
		RETURN TRUE;
	END;
CREATE PROCEDURE SelectDataFromDB ( )
BEGIN
	SET Environment.Data.Result[] = SELECT A.* FROM Database.{TABLE} AS A;
END;
END MODULE;
