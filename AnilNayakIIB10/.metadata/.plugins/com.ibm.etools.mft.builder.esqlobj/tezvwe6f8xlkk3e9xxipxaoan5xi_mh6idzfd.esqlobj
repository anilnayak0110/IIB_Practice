	CREATE PROCEDURE DateConvert() BEGIN		


		DECLARE source CHARACTER '20-12-17';
		DECLARE target DATE;
		DECLARE pattern CHARACTER 'mm-dd-yy';
		SET target = CAST(source AS DATE FORMAT pattern);
		SET OutputRoot = InputRoot;
	END;
