	CREATE PROCEDURE  UpdateDataUsingPassthru( )
	BEGIN
		DECLARE SQL CHARACTER;
		SET SQL = 'UPDATE EMPLOYEE SET ENAME = ? WHERE EID = ?';
		SET Environment.Data[] = PASSTHRU(SQL TO Database.{MySqlDataSource} VALUES('Gayatree',10));
	END;
	
