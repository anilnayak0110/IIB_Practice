

BROKER SCHEMA com.anilnayak.esb


CREATE FILTER MODULE filterFlow_Filter
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		
		RETURN FilterMessage();
	END;
	
	
	CREATE PROCEDURE FilterMessage( ) RETURNS BOOLEAN
